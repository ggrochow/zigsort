---
import Layout from '../layouts/Layout.astro';
---

<Layout title="Welcome to Astro.">
	<main>
    <div class="controls">
      <input type='file' id='file_input'>
    </div>
    <canvas id='canvas'>
	</main>
</Layout>

<style>

html, main, body {
  height: 100%;
  width: 100%;
  padding: 0;
  margin: 0;
}

#canvas {
  height: 95%;
  width: 95%;
  border: 1px solid red;
}
  
</style
